<div id="sylius-variants-recurring-times" data-unavailable-text="{{ 'sylius.ui.unavailable'|trans }}">
    {% for variant in product.getEnabledVariants() %}
        <div data-variant="{{ variant.code }}" {{ sylius_test_html_attribute('variant-times') }} data-value="{{ variant.times }}" ></div>
    {% endfor %}
</div>
<div id="sylius-variants-recurring-interval" data-unavailable-text="{{ 'sylius.ui.unavailable'|trans }}">
    {% for variant in product.getEnabledVariants() %}
            <div data-variant="{{ variant.code }}" {{ sylius_test_html_attribute('variant-interval') }} data-value="{{ variant.interval }}" ></div>
    {% endfor %}
</div>
